
generator client {
    provider = "prisma-client-js"
  }
  
  datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
  }
  
model Users {
  _id      String   @id @default(auto()) @map("_id") @db.ObjectId 
  email    String   @unique            
  name     String?                      
  password String?                      
}

model  
